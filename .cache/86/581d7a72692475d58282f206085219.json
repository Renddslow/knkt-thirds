      "sources": {
        "src/dotSermon.js": "const parse = (text) => {\n  const lines = text.split('\\n').reduce((acc, line) => line.trim() ? [...acc, line.trim()] : acc, []);\n\n  if (lines[0][0] !== '[') {\n    return null;\n  }\n\n  const slides = [];\n\n  let currentSlide = {};\n\n  for (let i = 0; i < lines.length; i++) {\n    if (lines[i] === '[verse]' || lines[i] === '[point]') {\n      if (currentSlide.type) {\n        slides.push(currentSlide);\n      }\n\n      currentSlide = {\n        type: lines[i].replace(/\\W/g, ''),\n      };\n      continue;\n    }\n\n    const words = lines[i].split(' ');\n    if (words.length > 2 && words[1] === '=') {\n      const key = words[0];\n      currentSlide[key] = words.slice(2).join(' ');\n      continue;\n    }\n\n    currentSlide.content = currentSlide.content ? `${currentSlide.content} ${lines[i]}` : lines[i];\n  }\n\n  slides.push(currentSlide);\n\n  return slides;\n};\n\nexport default parse;\n"
