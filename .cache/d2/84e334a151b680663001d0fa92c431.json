      "sources": {
        "src/styles.css": "* {\n    padding: 0;\n    margin: 0;\n}\n\nbody {\n    background: #dfdfdf;\n    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Oxygen', 'Ubuntu', 'Fira Sans', 'Droid Sans', 'Helvetica Neue', sans-serif;\n}\n\ncanvas#cvs {\n    width: 100%;\n    max-width: 960px;\n    margin: 24px auto 0;\n    display: block;\n    background-color: #fff;\n    background-image: linear-gradient(45deg, #808080 25%, transparent 25%), linear-gradient(-45deg, #808080 25%, transparent 25%), linear-gradient(45deg, transparent 75%, #808080 75%), linear-gradient(-45deg, transparent 75%, #808080 75%);\n    background-size: 20px 20px;\n    background-position: 0 0, 0 10px, 10px -10px, -10px 0;\n}\n\n.controls {\n    display: grid;\n    grid-template-columns: repeat(4, minmax(0, 1fr));\n    background: #fff;\n    position: absolute;\n    bottom: 0;\n    width: 100%;\n    box-sizing: border-box;\n    padding: 24px;\n}\n\n.controls section {\n    padding: 12px;\n    display: block;\n    box-sizing: border-box;\n}\n\n.controls section h2 {\n    font-size: 14px;\n    font-weight: 800;\n    margin-bottom: 12px;\n}\n\n.controls section:not(:last-child) {\n    border-right: 1px solid #efeff0;\n}\n\n.field {\n    display: grid;\n    grid-gap: 8px;\n    grid-template-columns: minmax(0, max-content) minmax(0, 1fr);\n    align-items: center;\n    margin-top: 8px;\n    position: relative;\n}\n\n.field.block {\n    grid-template-columns: minmax(0, 1fr);\n    grid-template-rows: repeat(2, minmax(0, max-content));\n}\n\ninput:not([type=\"file\"]) {\n    padding: 12px;\n    box-sizing: border-box;\n    border-radius: 8px;\n    border: 2px solid #dfdfdf;\n    font-size: 12px;\n    appearance: none;\n    -webkit-appearance: none;\n    outline: none;\n}\n\ninput:not([type=\"file\"]):focus {\n    border-color: #0052CC;\n}\n\n.field label {\n    font-weight: 600;\n    font-size: 14px;\n    display: block;\n    color: #898989;\n}\n\nbutton.icon {\n    background: transparent;\n    border-radius: 8px;\n    padding: 8px;\n    box-sizing: border-box;\n    display: flex;\n    align-items: center;\n    font-size: 14px;\n    color: #898989;\n    border: 0;\n    appearance: none;\n    -webkit-appearance: none;\n    cursor: pointer;\n}\n\nbutton.icon span:last-child {\n    margin-left: 8px;\n}\n\nbutton.icon:focus,\nbutton.icon:active,\nbutton.icon:hover {\n    background: #e2e2e2;\n    color: #494949;\n    outline: none;\n}\n\ninput[type=\"file\"] {\n    width: 0;\n    height: 0;\n    position: absolute;\n}\n\n.color-preview {\n    width: 24px;\n    height: 24px;\n    border: 2px solid #dfdfdf;\n    border-radius: 50%;\n    position: absolute;\n    background: #000;\n    right: 0;\n    bottom: 0;\n    margin: 8px;\n}\n\nbutton:not(.icon) {\n    border-radius: 8px;\n    padding: 12px 34px;\n    font-weight: 800;\n    box-sizing: border-box;\n    background: #0052CC;\n    color: #fff;\n    font-size: 16px;\n    border: 0;\n    appearance: none;\n    -webkit-appearance: none;\n    margin-bottom: 8px;\n    cursor: pointer;\n}\n\nbutton.primary {\n    background: #00875A;\n}\n\nbutton:disabled {\n    opacity: 0.6;\n    cursor: not-allowed;\n}\n\n@media screen and (max-width: 960px) {\n    .controls {\n        display: block;\n    }\n\n    .controls section:not(:last-child) {\n        border-right: 0;\n        border-bottom: 1px solid #efeff0;\n    }\n}\n\n"
